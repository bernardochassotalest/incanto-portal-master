{
  "name": "incanto-jobs",
  "version": "1.0.35",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "sucrase ./src -d ./dist --transforms imports ; cp src/app/lib/notification/firebase-key.json dist/app/lib/notification ; cp src/app/resources/email/* dist/app/resources/email",
    "dev": "DEBUG=incanto* npm-run-all -p dev:*",
    "test": "NODE_ENV=test DEBUG=incanto* npm-run-all -p test:*",
    "prod": "yarn build && npm-run-all -p prod:*",
    "dev:server": "DEBUG=incanto* nodemon src/index.js",
    "dev:queue": "DEBUG=incanto* nodemon src/queue.js",
    "test:server": "NODE_ENV=test DEBUG=incanto* sucrase-node src/index.js",
    "test:queue": "NODE_ENV=test DEBUG=incanto* sucrase-node src/queue.js",
    "prod:server": "cp -v -r src/app/resources dist/app && node ./dist/index.js",
    "prod:queue": "node ./dist/queue.js"
  },
  "dependencies": {
    "amqplib": "^0.6.0",
    "async": "^3.2.0",
    "axios": "^0.19.2",
    "bcrypt-nodejs": "0.0.3",
    "bull": "^3.11.0",
    "bull-board": "^0.5.0",
    "chardet": "^0.7.0",
    "credit-card-type": "^9.0.1",
    "currency.js": "^1.2.2",
    "date-fns": "^2.9.0",
    "date-fns-tz": "^1.0.10",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "exceljs": "^4.2.0",
    "express": "^4.17.1",
    "firebase": "^7.13.2",
    "firebase-admin": "^8.12.1",
    "firebase-functions": "^3.7.0",
    "firebase-tools": "^8.0.1",
    "intl": "^1.2.5",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongoose": "5.6.13",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.1",
    "odata-client": "^0.6.13",
    "pdf-image": "^2.0.0",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "request": "^2.88.2",
    "sequelize": "^5.21.5",
    "uuid": "^3.4.0",
    "winston": "^3.3.3",
    "xlsx": "^0.16.4"
  },
  "devDependencies": {
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "sucrase": "^3.10.1"
  }
}
